@model IEnumerable<Dashboard.Models.FormaPagamento>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.DescricaoFp)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TipoFp)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DescricaoFp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoFp)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdFormaPagamento">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdFormaPagamento">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdFormaPagamento">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
